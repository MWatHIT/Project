msgid "heading-purge-all-caches"
msgstr ""

#. Default: "Empty all caches and force reloading of user records from LDAP."
#: plone.app.ldap/plone/app/ldap/browser/controlpanel.pt:359
msgid "help-purge-cache"
msgstr ""

